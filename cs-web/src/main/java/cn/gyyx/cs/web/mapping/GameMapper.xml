<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.gyyx.cs.persist.GameMapper">
	<resultMap id="gameResultMap" type="cn.gyyx.cs.model.GameInfo">
		<id column="code" property="code" jdbcType="INTEGER" />
		<result column="name" property="name" jdbcType="VARCHAR" />
	</resultMap>
	<resultMap id="serverGameResultMap" type="cn.gyyx.cs.model.GameInfo">
		<id column="code" property="code" jdbcType="INTEGER" />
		<result column="game_name" property="name" jdbcType="VARCHAR" />
	</resultMap>
	<select id="getAllGame" resultMap="gameResultMap">
		select code,name
		from game_tb
	</select>
</mapper>